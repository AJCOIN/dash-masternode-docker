{
  "network": "testnet",
  "port": 3001,
  "services": [
    "dashd",
    "web",
    "@dashevo/insight-api"
  ],
  "servicesConfig": {
    "dashd": {
      "connect": [
        {
          "rpchost": "dashd",
          "rpcport": 20002,
